<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.atguigu.web.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/userServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>S_infoServlet</servlet-name>
        <servlet-class>com.atguigu.web.S_infoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>S_infoServlet</servlet-name>
        <url-pattern>/s_infoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ScoreListServlet</servlet-name>
        <servlet-class>com.atguigu.web.ScoreListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreListServlet</servlet-name>
        <url-pattern>/scoreListServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TeacherServlet</servlet-name>
        <servlet-class>com.atguigu.web.TeacherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherServlet</servlet-name>
        <url-pattern>/teacherServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RootServlet</servlet-name>
        <servlet-class>com.atguigu.web.RootServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RootServlet</servlet-name>
        <url-pattern>/rootServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ScoreManagerServlet</servlet-name>
        <servlet-class>com.atguigu.web.ScoreManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScoreManagerServlet</servlet-name>
        <url-pattern>/scoreManagerServlet</url-pattern>
    </servlet-mapping>

</web-app>